@{
    Layout = null;
}

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>安管人员考核系统-用户登录</title>
    <link href="~/Public/content/login/css/login.css" rel="stylesheet" />
</head>

<body>
    <div class="container">
        <div class="content_left"></div>
        <div class="content_right">
            <div class="title"><img src="~/Public/content/login/images/logo.png" width="224" height="58"></div>
            <div id="divLogin_AccountInfo" class="content_form">
                <input name="loginname" type="text" class="username" placeholder="请输入用户名">
                <input name="password" type="password" class="username" placeholder="请输入密码">
                <button id="btnLogin" class="content_btn">立即登录</button>
            </div>
        </div>
    </div>

    <script src="~/Public/jquery/js/jquery-1.10.2.min.js"></script>
    <script src="~/Public/base/jsFn.js"></script>
    <script src="~/Public/md5/js/md5.js"></script>
    <script src="~/Public/content/Login/js/login.js?v=@DateTime.Now.ToString("yyyyMMddHHmmss")"></script>

</body>
</html>